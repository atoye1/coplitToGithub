{
  "manifest_version": 3,
  "name": "Coplit To Git",
  "version": "1.0",
  "description": "Automatically commit coplit codes to github after submission",
  "action": {
    "default_popup": "hello.html"
  },
  "content_scripts": [
    {
      "js": ["scripts/content.js"],
      "matches": ["https://urclass.codestates.com/codeproblem/*"]
    }
  ]
}
